{
  "name": "boilerplate-material-ui-webpack",
  "version": "0.0.1",
  "description": "a boilerplate project for reactj, material-ui and webpack",
  "main": "main.js",
  "scripts": {
    "build": "BUILD_DEV=1 webpack --config webpack.build.config.js",
    "dev": "BUILD_DEV=1 webpack-dev-server --port 5050 --devtool eval --progress --colors --hot --content-base app",
    "deploy": "NODE_ENV=production webpack -p --config webpack.production.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lionkeng/boilerplate-material-ui-webpack.git"
  },
  "keywords": [
    "reactjs",
    "material-ui",
    "webpack"
  ],
  "author": "Keng Lim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lionkeng/boilerplate-material-ui-webpack/issues"
  },
  "homepage": "https://github.com/lionkeng/boilerplate-material-ui-webpack",
  "dependencies": {
    "babel-core": "^5.5.0",
    "babel-loader": "^5.1.3",
    "css-loader": "^0.14.4",
    "file-loader": "^0.8.4",
    "jsx-loader": "^0.13.2",
    "material-ui": "^0.8.0",
    "node-libs-browser": "^0.5.2",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10"
  },
  "devDependencies": {
    "webpack-dev-server": "^1.9.0"
  }
}
